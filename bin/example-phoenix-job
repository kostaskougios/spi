
JAR=target/phoenix.jar

spark-submit --class com.aktit.phoenix.SamplePopulateJob \
	--driver-memory 1g \
	--executor-memory 1g \
	--conf spark.yarn.executor.memoryOverhead=1024 \
	--executor-cores 4 \
	--num-executors 3 \
	--master yarn-cluster \
	--conf spark.num-of-rows=1000000000000 \
	--conf spark.hbase.zookeeper=nn.lan \
	$JAR
